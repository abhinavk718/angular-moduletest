package com.ty.java8features.predicate;

import java.util.function.Predicate;

public class TestC
{
	public static void main(String[] args)
	{
		Predicate<Integer> p = j -> {
			if(j%5==0)
			{
				return true;
			}
			else
			{
				return false;
			}
		};
		boolean res=p.test(24);	
		System.out.println("Result "+res);
		
	}
}
